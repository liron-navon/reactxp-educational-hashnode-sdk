{
  "name": "reactxp-educational-hashnode-sdk",
  "description": "A native multi platform SDK built with reactxp for a hashnode post",
  "version": "0.0.1",
  "private": false,
  "main": "dist/index.js",
  "types": "./src/index.d.ts",
  "dependencies": {
    "reactxp": "^1.6.1",
    "reactxp-virtuallistview": "^2.0.0-rc.3"
  },
  "scripts": {
    "start": "npm run storybook",
    "storybook": "cross-env RN_PLATFORM=web start-storybook -p 9009",
    "storybook:build": "cross-env RN_PLATFORM=web build-storybook",
    "build": "webpack",
    "build:ios": "cross-env RN_PLATFORM=ios npm run build",
    "build:android": "cross-env RN_PLATFORM=android npm run build",
    "build:web": "cross-env RN_PLATFORM=web npm run build",
    "build:all": "npm run build:ios && npm run build:android && npm run build:web",
    "build:publish": "rm -rf ./dist && cross-env NODE_ENV=production npm run build:all && node scripts/pre-publish.js",
    "test:web": "cross-env RN_PLATFORM=web jest --config='jest.config.web.js'",
    "test:mobile": "jest --config='jest.config.mobile.js'",
    "test": "jest"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "jest": {
    "projects": [
      "jest.config.web.js",
      "jest.config.mobile.js"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@babel/plugin-proposal-class-properties": "^7.3.4",
    "@babel/plugin-proposal-export-default-from": "^7.2.0",
    "@babel/preset-env": "^7.4.2",
    "@babel/preset-react": "^7.0.0",
    "@storybook/addon-a11y": "^5.0.5",
    "@storybook/addon-actions": "^5.0.5",
    "@storybook/addon-info": "^5.0.5",
    "@storybook/addon-links": "^5.0.5",
    "@storybook/addon-storysource": "^5.0.5",
    "@storybook/addons": "^5.0.5",
    "@storybook/react": "^5.0.5",
    "@svgr/webpack": "^4.1.0",
    "@types/react": "^16.8.8",
    "@types/react-dom": "^16.8.3",
    "@types/react-native": "^0.57.41",
    "@types/react-test-renderer": "^16.8.1",
    "awesome-typescript-loader": "^5.2.1",
    "babel-cli": "^6.26.0",
    "babel-jest": "^24.5.0",
    "babel-loader": "^8.0.5",
    "babel-plugin-module-resolver": "^3.2.0",
    "babel-plugin-named-asset-import": "^0.3.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-react-app": "^7.0.2",
    "babel-preset-react-native": "^4.0.1",
    "clean-webpack-plugin": "^2.0.1",
    "cross-env": "^5.2.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.10.0",
    "enzyme-to-json": "^3.3.5",
    "eslint": "^5.15.1",
    "fs-extra": "^7.0.1",
    "image-size": "^0.7.2",
    "jest": "^24.5.0",
    "json-loader": "^0.5.7",
    "lodash": "^4.17.11",
    "metro-react-native-babel-preset": "^0.53.0",
    "react": "^16.8.6",
    "react-docgen-typescript-loader": "^3.0.1",
    "react-dom": ">=16.5.1",
    "react-markdown": "^4.0.6",
    "react-native": "^0.57.8",
    "react-native-webpack": "^0.1.1",
    "react-scripts": "^2.1.8",
    "react-syntax-highlighter": "^10.2.1",
    "react-test-renderer": "^16.8.5",
    "reactxp": "^1.6.1",
    "source-map-loader": "^0.2.4",
    "ts-jest": "^24.0.0",
    "typescript": "^3.1.6",
    "url-loader": "^1.1.2",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.2.1",
    "webpack-merge": "^4.2.1"
  }
}
